export PATH="/bin:/sbin:/usr/bin:/usr/sbin:/progs:/files/x86_64-linux-musl-native/bin:/files/Python-3.11.3/bin"

if [ "$PS1" ]; then
	if [ "`id -u`" -eq 0 ]; then
		export PS1='\[\e[1;32m\]\u\[\e[1;37m\]@\[\e[36m\]\h\[\e[1;34m\] \w\[\e[1;31m\] #\[\e[m\] '
	else
		export PS1='$ '
	fi
fi

export EDITOR='/bin/vi'

# Source configuration files from /etc/profile.d
for i in /etc/profile.d/*.sh ; do
	if [ -r "$i" ]; then
		. $i
	fi
done
unset i
